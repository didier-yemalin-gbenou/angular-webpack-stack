{
  "name": "angular-webpack-stack",
  "version": "0.1.0",
  "description": "A sample project showcasing Yarn, webpack and TypeScript with React for the Linux.com article series.",
  "main": "src/index.js",
  "license": "Apache 2.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:kenzanlabs/angular-webpack-stack.git"
  },
  "keywords": [
    "kenzanlabs",
    "Angular",
    "Angular 2",
    "Webpack",
    "seed",
    "Bootstrap",
    "Bootstrap 4",
    "TypeScript"
  ],
  "scripts": {
    "build": "npm run clean && npm run lint && webpack --config ./webpack.config.prod.js -p",
    "clean": "rimraf ./build && rimraf ./reports && mkdir ./reports && mkdir ./reports/test-results",
    "develop": "webpack-dev-server --open --config ./webpack.config.develop.js",
    "lint": "eslint *.js",
    "serve": "export NODE_ENV=production && npm run build && http-server ./build -o -p 9000",
    "test": "npm run clean && karma start ./karma.config.js"
  },
  "dependencies": {
